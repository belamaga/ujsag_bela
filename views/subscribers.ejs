<!DOCTYPE html>
<html lang="hu">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Előfizetők</title>
        <link rel="stylesheet" href="/css/subscribers.css">
    </head>
    <body>
        <div id="subscribers-kontener">
        <%- include('nav.ejs') %>
            <h1>Előfizetők</h1>
            <ul>
                <% for( let i = 0; i < subscribers.length; i++ ) { %>
                    <% if (subscribers[i].status === true) { %>
                        <li>Előfizető neve: <%= subscribers[i].nev %></li>
                    <% } %>
                <% } %>
            </ul>
        </div>
    </body>
</html>
